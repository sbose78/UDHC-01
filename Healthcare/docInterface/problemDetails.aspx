<%@ Page Title="" Language="C#" MasterPageFile="~/Site.Master" AutoEventWireup="true" CodeBehind="problemDetails.aspx.cs" Inherits="Healthcare.docInterface.WebForm4" %><asp:Content ID="Content1" ContentPlaceHolderID="HeadContent" runat="server">

    <%@ Import Namespace="System.Data" %>


    <style type="text/css">
        .style1
        {
            font-size: large;
            text-align: center;
        }
    </style>
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="MainContent" runat="server">
    <%
    DataTable dt = getAllData();
    int i = 0;   
    
 %>

    <div>
        <font face="trebuchet ms,sans-serif">1. Name (autogenerated)
        <br />

        

        <%=dt.Rows[0][i++].ToString() %>

        </font>
    </div>
    <div>
        <font face="trebuchet ms,sans-serif">2. Age<br />

          <%=dt.Rows[0][i++].ToString() %>
        <br />
        3. Gender (very important)<br />
          <%=dt.Rows[0][i++].ToString() %>
        </font>
    </div>
    <div>
        <font face="trebuchet ms,sans-serif">4. Marital status and number of 
        abortions/children (esp for female patients)&nbsp;<br />
          <%=dt.Rows[0][i++].ToString() %>
        </font>
    </div>
    <div>
        <font face="trebuchet ms,sans-serif">5.</font><span 
            style="font-family:'trebuchet ms',sans-serif">What is your main health issue<br />
              <%=dt.Rows[0][i++].ToString() %>
        <br />
        </span>
    </div>
    <div class="style2">
          
    </div>
    <div>
        <font face="'trebuchet ms', sans-serif">a. Duration of complaint:<br />
        </font>
        <%=dt.Rows[0][i++].ToString() %>
    </div>
    <div>
        <font face="'trebuchet ms', sans-serif">b. Describe the problem:<br />
        <br />
        <%=dt.Rows[0][i++].ToString() %>
        </font>
    </div>
    <div>
        <font face="'trebuchet ms', sans-serif">c. To what degree is it interfering with 
        your life -

        <br />
        <%=dt.Rows[0][i++].ToString() %>
        <br />
        <%=dt.Rows[0][i++].ToString() %>
        Comments?<br />
        </font>
    </div>
    <div>
        <font face="'trebuchet ms', sans-serif">d. Are you currently seeking any 
        treatment for it (allopathic or alternative):<br />
        <%=dt.Rows[0][i++].ToString() %>
        </font>
    </div>
    <div class="style1">
        <font face="'trebuchet ms', sans-serif">&nbsp;Past history</font></div>
        <%=dt.Rows[0][i++].ToString() %>
    <div>
        <font face="'trebuchet ms', sans-serif">&nbsp;1. Have you experienced similar 
        problems in the past ?&nbsp;<br />
        <%=dt.Rows[0][i++].ToString() %>
        <br />
        </font>
    </div>
    <div>
        <font face="'trebuchet ms', sans-serif">a. If yes, did you take any treatment 
        for it? Did it help?<br />

        <%=dt.Rows[0][i++].ToString() %>
        <br />
        </font>
    </div>
    <div>
        <font face="'trebuchet ms', sans-serif">2. Have you had any major illnesses in 
        the past which required hospitalization or a trip to the physician<br />
        <br />
        <%=dt.Rows[0][i++].ToString() %>
        </font>
    </div>
    <div>
        <font face="trebuchet ms,sans-serif">3. Any long term (chronic) health 
        conditions you have been diagnosed with or on treatment for (diabetes, heart 
        conditions, HTN)<br />
        <%=dt.Rows[0][i++].ToString() %>
        <br />
        </font>
    </div>
    <div>
        <font face="trebuchet ms,sans-serif">4. Any allergies you are aware of ?<br />
        <br />
        <%=dt.Rows[0][i++].ToString() %>
        </font>
    </div>
    <div>
        <font face="trebuchet ms,sans-serif">5. Does anyone in your immediate family 
        have a similar condition (as your main complaint)<br />
        <%=dt.Rows[0][i++].ToString() %>
        <br />
        </font>
    </div>
    <div>
        <font face="trebuchet ms,sans-serif">6. Does anyone in your family have any 
        chronic health issue (HTN/CVS/CNS/Mental health issues/Skin problems etc)<br />
        <%=dt.Rows[0][i++].ToString() %>
        <br />
        </font>
    </div>
    <p>
       
    </p>
</asp:Content>
